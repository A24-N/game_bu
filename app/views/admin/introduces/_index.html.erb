<table class="table" style="table-layout:fixed;">
  <thead>
    <tr>
      <th>ID.Fromユーザー</th>
      <th>ID.Toユーザー</th>
      <th></th>
    </tr>
  </thead>
  <% introduces.each do |introduce| %>
    <tr>
      <td><%= introduce.introduce_from_user.id %>.<%= introduce.introduce_from_user.nickname %></td>
      <td><%= introduce.introduce_to_user.id %>.<%= introduce.introduce_to_user.nickname %></td>
      <td><%= link_to "詳細ページ", admin_introduce_path(introduce) %></td>
    </tr>
  <% end %>
</table>